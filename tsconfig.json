{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    ".eslintrc.js",
    "vite.config.ts"
  ],
  "vueCompilerOptions": {
    "strictTemplates": true
  },
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": ["./node_modules/element-plus/global.d.ts"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}